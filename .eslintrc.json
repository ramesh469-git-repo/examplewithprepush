{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "airbnb"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2020,
      "type": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "react/prop-types": "off"
    }
  }
  