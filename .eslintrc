{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "airbnb"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2020,
      "sourceType": "module",
      "allowImportExportEverywhere": true
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-unused-vars": ["warn",{ "varsIgnorePattern": "^React$"}],
      "react/prop-types": "off"
    }
  }
  